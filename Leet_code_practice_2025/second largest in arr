class Solution {
public:
    int secondLargestElement(vector<int>& nums) {
        //your code goes here
        int secondLargest=INT_MIN;
        int largest=INT_MIN;
        for(int x : nums){
            if(x>largest){
                secondLargest= largest;
                largest=x;
                
            }
            else if(x >secondLargest && x!=largest){
                secondLargest=x;
            }




        }
        if(secondLargest==INT_MIN){
            return -1;
        }
      return secondLargest;
    }
};
